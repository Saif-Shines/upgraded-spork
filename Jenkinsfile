            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployeing'
            }
        }
        stage('Test') {
            steps {
                echo 'TEsting'
            }
        }
        stage('RElease') {
            steps {
                echo 'Release'
            }
        }
    }
}
